{
  "name": "vitimchat",
  "version": "0.0.1",
  "private": true,
  "description": "Vitimchat monorepo",
  "license": "ISC",
  "author": "vitim",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "ci": "lerna run ci",
    "clean:sdk": "lerna exec --scope @vitim-sdk/* \"n-clean \"dist\"\"",
    "depcheck": "lerna exec \"depcheck --ignore-dirs=dist\"",
    "prepublish:sdk": "cross-env NODE_OPTIONS=--max_old_space_size=12000 lerna run --scope @vitim-sdk/* build",
    "publish:sdk": "lerna publish from-git --target dist --no-verify-access --yes",
    "release": "lerna version",
    "version": "auto-changelog -v v$(node -e \"console.log(require('./lerna.json').version)\") && git add CHANGELOG.md && node scripts/version.js",
    "postinstall": "lerna run --scope @vitim-sdk/* init",
    "clean:dependencies:packages": "lerna exec \"n-clean node_modules package-lock.json\"",
    "clean:dependencies": "npm run clean:dependencies:packages && n-clean node_modules package-lock.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "auto-changelog": "^1.11.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "depcheck": "^0.6.11",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.0",
    "lerna": "^3.15.0",
    "mocha": "^5.2.0",
    "npm-build-tools": "^2.2.5",
    "patch-package": "^5.1.0"
  },
    "react-tiny-virtual-list": "^2.2.0"
  }
}
